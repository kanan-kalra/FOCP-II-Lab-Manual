#include <iostream>
using namespace std;
int main()
{
    int n;
    cout << "enter the number of entries:";
    cin >> n;
    int i, largest, a;
    cout << "Enter the 1st sale :";
    cin >> a;
    largest = a;
    int b;
    for (i = 2; i <= n; i++)
    {
        cout << "enter the " << i << " sale:";
        cin >> b;
        if (b > largest)
        {
            largest = b;
        }
    }
    cout << "maximum sale is:" << largest;
    return 0;
}
