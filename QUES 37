#include <iostream>
#include <string>
using namespace std;
int main()
{
    string username;
    cout << "Enter username: ";
    cin >> username;

    for (int i = 0; i < username.length(); i++)
    {
        if (!((username[i] >= 'A' && username[i] <= 'Z') ||
              (username[i] >= 'a' && username[i] <= 'z') ||
              (username[i] >= '0' && username[i] <= '9')))
        {
            cout << "Invalid username";
            return 0;
        }
    }

    cout << "Username accepted";
    return 0;
}
